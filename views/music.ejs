<%- include("partials/header.ejs") %> 
<div class="container top-music">
<div class="row">
  <% allMusic.map((music) => {%>
    <div class="col s6 m6 l4">
<div class="card "style="height: 50%"  >
    <div class="card-image waves-effect waves-block waves-light">
      <img class="activator" src=<%= music.photoDetails.url %>  >
    </div>
    <div class="card-content cont">
      <span class="card-title activator grey-text text-darken-4 center card-font "> <%= music.title %> </span>
    </div>
    <div class="card-reveal">
      <span class="card-title grey-text text-darken-4 center"> <i class="fas fa-times right"></i></span>
      <br>
      <ul >
        <li class="center"><a target="_blank" href=<%= music.youtubeLink %> > <i class="fab fa-youtube fa-4x"></i></a></li> <br>
        <li class="center"><a target="_blank" href=<%= music.spotifyLink %> > <i class="fab fa-spotify fa-4x "></i></a></li> <br>
        <li class="center"><a target="_blank" href=<%= music.soundcloudLink %> ><i class="fab fa-soundcloud fa-4x"></i></a></li> <br>
        <li class="center"><a target="_blank" href=<%= music.applemusicLink %> ><i class="fab fa-apple fa-4x"></i></a></li>
    </ul>
    </div>
  </div>
</div>
<% }); %>


  

<!-- end -->
</div>
</div>
<%- include("partials/footer.ejs") %> 